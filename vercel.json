{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/admin",
            "destination": "/public/admin/index.html"
        },
        {
            "source": "/blog-post/(.*)",
            "destination": "/blog-post/charity-meals-that-change-the-lives-every-day.html"
        },
        {
            "source": "/",
            "destination": "/index.htm"
        }
    ]
}
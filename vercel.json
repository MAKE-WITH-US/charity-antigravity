{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "public/**",
                    "data/**"
                ]
            }
        },
        {
            "src": "**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.htm",
            "use": "@vercel/static"
        },
        {
            "src": "68*/**",
            "use": "@vercel/static"
        },
        {
            "src": "images/**",
            "use": "@vercel/static"
        },
        {
            "src": "ajax/**",
            "use": "@vercel/static"
        },
        {
            "src": "favicon/**",
            "use": "@vercel/static"
        },
        {
            "src": "gsap/**",
            "use": "@vercel/static"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "data/**",
            "use": "@vercel/static"
        },
        {
            "src": "service/**",
            "use": "@vercel/static"
        },
        {
            "src": "blog-post/**",
            "use": "@vercel/static"
        },
        {
            "src": "blog-category/**",
            "use": "@vercel/static"
        },
        {
            "src": "causes/**",
            "use": "@vercel/static"
        },
        {
            "src": "template-info/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/admin",
            "dest": "/server.js"
        },
        {
            "src": "/admin/(.*)",
            "dest": "/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}